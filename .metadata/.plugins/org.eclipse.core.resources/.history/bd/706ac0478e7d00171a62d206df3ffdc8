import numpy as np
import serial
import time

class ArduinoSolDev(object):
    '''
    classdocs
    '''

    def __init__(self, port='COM3',baud_rate=250000):
        '''
        Constructor
        '''
        self.port=port
        self.baud_rate=baud_rate
        
        self.ser=serial.Serial(self.port,self.baud_rate,timeout=1)
        
        
    def write(self,write):
    def write_ser(self,val):
        self.ser.write(val)
        
    def read(self):
        return self.ser.readline()
    
    def open(self):
        self.ser.open()
        
    def close(self):
        self.ser.close()
    
    def __del__(self):
        self.close()
        del self.ser
        

if __name__ == '__main__':
    sol=ArduinoSolDev()
    time.sleep(2)
    a=time.time()
    for i in range(1000):
        sol.write_ser('134,25,556,65\r\n')
    b=time.time()
    print(sol.read(),i)
    
    print(b-a)