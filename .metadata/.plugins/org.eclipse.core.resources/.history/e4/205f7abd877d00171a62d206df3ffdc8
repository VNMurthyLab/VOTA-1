import numpy as np
import serial

class ArduinoSolDev(object):
    '''
    classdocs
    '''

    def __init__(self, port='COM3',baud_rate=115200):
        '''
        Constructor
        '''
        self.port=port
        self.baud_rate=baud_rate
        
        self.ser=serial.Serial(self.port,self.baud_rate,timeout=1)